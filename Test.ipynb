    "from datetime import date, timedelta\n",
    "\n",
    "DATE_FORMAT = \"%Y-%m-%d\"  # Define your desired format here\n",
    "today = date.today() + timedelta(days=1)\n",
    "today_str = f\"{today.strftime(DATE_FORMAT)} ({today.strftime('%a')})\"\n",
    "print(today_str)"
   ]
